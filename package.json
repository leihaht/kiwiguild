{
  "name": "kiwi-guild-site",
  "version": "0.0.1",
  "description": "Kiwi guild web site",
  "repository": {
    "type": "git",
    "url": "git://github.com/leihaht/kiwiguild.git"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browserify": "~14.1.0",
    "browserify-shim": "~3.8.14",
    "coffee-script": "~1.12.4",
    "coffeeify": "~2.1.0",
    "connect": "~3.6.0",
    "glob": "^7.1.1",
    "gulp": "~3.9.1",
    "gulp-changed": "~2.0.0",
    "gulp-clean": "~0.3.2",
    "gulp-csso": "~3.0.0",
    "gulp-imagemin": "~3.2.0",
    "gulp-less": "~3.3.0",
    "gulp-livereload": "~3.8.1",
    "gulp-notify": "~3.0.0",
    "gulp-open": "~2.0.0",
    "gulp-util": "~3.0.8",
    "merge-stream": "^1.0.1",
    "path": "^0.12.7",
    "vinyl-source-stream": "~1.1.0"
  },
  "dependencies": {
    "jquery": "~3.2.0"
  }
}
